msgid "Max Retries"
msgstr "最大重试次数"

msgid "Specify maximum retries when using 'on-failure' restart policy"
msgstr "当重启策略为'on-failure'时，指定最大重试次数"

msgid "Restart Policy (--restart)"
msgstr "重启策略 (--restart)"

msgid "Restart Policy: Do not restart"
msgstr "重启策略：不重启"

msgid "Restart Policy: Unless stopped"
msgstr "重启策略：除非手动停止"

msgid "Restart Policy: Always"
msgstr "重启策略：始终重启"

msgid "Restart Policy: On failure"
msgstr "重启策略：仅在失败时重启"

