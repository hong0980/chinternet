msgid "%s Configuration File"
msgstr "%s 配置文件"

msgid "Command failed"
msgstr "命令失败"

msgid "Failed to execute \"/etc/init.d/%s %s\" action: %s"
msgstr "无法执行 \"/etc/init.d/%s %s\" 操作: %s"

msgid "Enabled"
msgstr "已启用"

msgid "Disabled"
msgstr "禁用"

msgid "Textarea not found: %s"
msgstr "未找到文本区域: %s"

msgid "Contents of %s have been saved."
msgstr "%s 内容已保存。"

msgid "Service %s reloaded successfully."
msgstr "%s 服务重载成功。"

msgid "Service reload failed: %s"
msgstr "服务重载失败: %s"

